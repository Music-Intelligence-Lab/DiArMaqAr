{
  "name": "arabic-maqam-core",
  "version": "1.0.1",
  "description": "Core library for Arabic maqam theory containing data, models, and utility functions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./models": {
      "import": "./dist/models/index.js",
      "require": "./dist/models/index.js",
      "types": "./dist/models/index.d.ts"
    },
    "./functions": {
      "import": "./dist/functions/index.js",
      "require": "./dist/functions/index.js",
      "types": "./dist/functions/index.d.ts"
    },
    "./data": {
      "import": "./dist/data/index.js",
      "require": "./dist/data/index.js",
      "types": "./dist/data/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "arabic",
    "maqam",
    "music",
    "theory",
    "microtonal",
    "jins",
    "tuning"
  ],
  "author": "Music Intelligence Lab",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Music-Intelligence-Lab/maqam-network.git",
    "directory": "packages/arabic-maqam-core"
  },
  "bugs": {
    "url": "https://github.com/Music-Intelligence-Lab/maqam-network/issues"
  },
  "homepage": "https://github.com/Music-Intelligence-Lab/maqam-network#readme"
}
