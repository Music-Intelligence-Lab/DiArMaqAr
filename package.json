    "js-yaml": "^4.1.0",
    "process": "^0.11.10",
    "sass": "^1.89.0",
    "stream-json": "^1.9.1",
    "typescript": "^5",
    "vitepress": "^1.6.4",
    "vitepress-openapi": "^0.1.10",
    "vitepress-plugin-llms": "^1.9.0"
