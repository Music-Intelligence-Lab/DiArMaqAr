    "next": "^15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonic-weave": "^0.11.0",
    "sw-synth": "^0.1.2",
    "tsx": "^4.20.6",
    "vexflow": "^5.0.0",
    "webmidi": "^3.1.12",
    "xen-dev-utils": "^0.11.2"
  },
  "devDependencies": {
